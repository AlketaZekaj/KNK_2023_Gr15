<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllerat.HyrjaController">
    <children>
        <Pane prefHeight="466.0" prefWidth="655.0" style="-fx-background-color: white;">
            <children>
                <Label fx:id="label1" alignment="CENTER" layoutX="202.0" layoutY="202.0" prefHeight="31.0" prefWidth="252.0" style="-fx-font-size: 20;" text="%label1" textFill="#201d1d" textOverrun="CENTER_ELLIPSIS">
                    <font>
                        <Font size="20.0" />
                    </font></Label>
                <Label layoutX="237.0" layoutY="253.0" prefHeight="26.0" prefWidth="128.0" style="-fx-font-size: 17;" text="%label2" />
                <Button alignment="CENTER" layoutX="237.0" layoutY="288.0" mnemonicParsing="false" onAction="#onStudentClick" prefHeight="31.0" prefWidth="156.0" style="-fx-background-color: red; -fx-text-fill: 'F0E2E7';" text="Student">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <Button alignment="CENTER" layoutX="237.0" layoutY="332.0" mnemonicParsing="false" onAction="#onProffesorClick" prefHeight="31.0" prefWidth="156.0" style="-fx-background-color: red; -fx-text-fill: 'F0E2E7';" text="Profesor">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <ImageView fitHeight="154.0" fitWidth="166.0" layoutX="245.0" layoutY="31.0">
                    <image>
                        <Image url="@../images/fiek%20-%20Copy.jpeg" />
                    </image>
                </ImageView>
                <MenuBar onKeyPressed="#onProffesorClick" prefHeight="26.0" prefWidth="655.0" style="-fx-background-color: red;">
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem fx:id="closeButton" mnemonicParsing="false" onAction="#onCloseClick" text="Close" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#onHelpClick" text="About Us" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <CheckBox fx:id="checkbox" layoutX="238.0" layoutY="375.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="227.0" text="%label3">
                    <font>
                        <Font size="13.0" />
                    </font></CheckBox>
                <Label fx:id="provalabel" layoutX="239.0" layoutY="409.0" prefHeight="17.0" prefWidth="267.0" textFill="#c72020" />
                <Button fx:id="albbtn" layoutX="275.0" layoutY="434.0" mnemonicParsing="false" onAction="#onalbbtnclick" text="ALB" />
                <Button fx:id="engbtn" layoutX="330.0" layoutY="434.0" mnemonicParsing="false" onAction="#onengbtnclick" text="ENG" />
            </children>
        </Pane>
    </children>
</AnchorPane>
