                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <Label layoutX="488.0" layoutY="258.0" prefHeight="53.0" prefWidth="32.0" text="FIEK" />
                <ImageView fitHeight="199.0" fitWidth="200.0" layoutX="398.0" layoutY="59.0">
                    <image>
                        <Image url="@../../../../OneDrive/Desktop/fiek.jpeg" />
                    </image>
                </ImageView>
                <MenuBar onKeyPressed="#onProffesorClick" prefHeight="26.0" prefWidth="655.0" style="-fx-background-color: #AAC6CD;">
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem fx:id="closeButton" mnemonicParsing="false" onAction="#onCloseClick" text="Close" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#onHelpClick" text="About Us" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <CheckBox fx:id="checkbox" layoutX="136.0" layoutY="391.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="227.0" text="%label3" />
                <Label fx:id="provalabel" layoutX="121.0" layoutY="416.0" prefHeight="17.0" prefWidth="267.0" textFill="#c72020" />
                <Button fx:id="albbtn" layoutX="273.0" layoutY="433.0" mnemonicParsing="false" onAction="#onalbbtnclick" text="ALB" />
                <Button fx:id="engbtn" layoutX="328.0" layoutY="433.0" mnemonicParsing="false" onAction="#onengbtnclick" text="ENG" />
            </children>
        </Pane>
    </children>
