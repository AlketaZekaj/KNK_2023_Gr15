<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ProvaController">
    <children>
        <Pane prefHeight="466.0" prefWidth="655.0" style="-fx-background-color: #C8E3EA;">
            <children>
                <Label fx:id="label1" layoutX="96.0" layoutY="77.0" prefHeight="31.0" prefWidth="252.0" style="-fx-font-size: 20;" text="%label1" textFill="#201d1d" textOverrun="CENTER_ELLIPSIS">
                    <font>
                        <Font size="20.0" />
                    </font></Label>
                <Label layoutX="158.0" layoutY="207.0" prefHeight="26.0" prefWidth="128.0" style="-fx-font-size: 17;" text="%label2" />
                <Button alignment="CENTER" layoutX="136.0" layoutY="277.0" mnemonicParsing="false" onAction="#onStudentClick" prefHeight="31.0" prefWidth="156.0" style="-fx-background-color: '1B62C5'; -fx-text-fill: 'F0E2E7';" text="Student">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <Button alignment="CENTER" layoutX="136.0" layoutY="330.0" mnemonicParsing="false" onAction="#onProffesorClick" prefHeight="31.0" prefWidth="156.0" style="-fx-background-color: '1B62C5'; -fx-text-fill: 'F0E2E7';" text="Profesor">
